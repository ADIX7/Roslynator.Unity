    </Samples>
  </Analyzer>
  <Analyzer Identifier="AddNewLineBeforeOperatorOfMultilineConditionalExpression"> <!--FormatConditionalExpression-->
    <Id>RCS1184</Id>
    <Title>Add new line before operator of multi-line conditional expression.</Title>
    <MessageFormat>Add new line before operator of multi-line conditional expression.</MessageFormat>
    <Category>Formatting</Category>
    <DefaultSeverity>Info</DefaultSeverity>
    <IsEnabledByDefault>false</IsEnabledByDefault>
    <SupportsFadeOut>false</SupportsFadeOut>
    <SupportsFadeOutAnalyzer>false</SupportsFadeOutAnalyzer>
    <Samples>
      <Sample>
        <Before><![CDATA[var y = (x) ? // [|Id|]
    Foo() :
    Bar();]]></Before>
        <After><![CDATA[var y = (x)
    ? Foo()
    : Bar();]]></After>
      </Sample>
    </Samples>
  </Analyzer>
</Analyzers>